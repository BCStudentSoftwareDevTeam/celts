from app.models import *
from app.models.user import User
from app.models.user import CertificationRequirement

program = ForeignKeyField(CertificationRequirement)
course = ForeignKeyField(CertificationRequirement)
username = CharField(primary_key = True)
term = ForeignKeyField(CertificationRequirement)
requirement = ForeignKeyField(CertificationRequirement)
addedBy = ForeignKeyField(user)
addedOn = ForeignKeyField(CertificationRequirement)
